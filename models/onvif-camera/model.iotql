--
--  compile to JSON with 'iotql-model model.iotql'
--
CREATE MODEL ONVIFCamera WITH
    schema:name = "ONVIF Camera"
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean,
    iot:write = true,
    iot:read = true,
    iot:actuator = true,
    iot:sensor = true
;
